<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zhoujiulong.chartsample">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.zhoujiulong.chartsample.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.zhoujiulong.chartsample.BarChartActivity" />
        <activity android:name="com.zhoujiulong.chartsample.LineChartActivity" />
        <activity android:name="com.zhoujiulong.chartsample.CurveChartActivity" />
        <activity android:name="com.zhoujiulong.chartsample.PieChartActivity" />
    </application>

</manifest>